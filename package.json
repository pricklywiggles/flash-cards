{
  "name": "flash-cards",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev --port 3001",
    "turbodev": "prisma generate && next dev --port 3001 --turbo",
    "build": "prisma generate && prisma db&& next build",
    "start": "next start",
    "lint": "next lint",
    "migrate": "dotenv -e .env.development.local -- npx prisma db push --schema src/prisma/schema.prisma",
    "db:seed": "dotenv -e .env.development.local prisma db seed",
    "db:reset": "dotenv -e .env.development.local -- npx prisma db push --force-reset",
    "db:push": "dotenv -e .env.development.local prisma db push",
    "db:pull": "dotenv -e .env.development.local prisma db pull",
    "db:generate": "dotenv -e .env.development.local prisma generate "
  },
  "prisma": {
    "schema": "src/prisma/schema.prisma",
    "seed": "dotenv -e .env.development.local -- ts-node -O {\"module\":\"commonjs\"} src/prisma/seed.ts"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.16",
    "@heroicons/react": "^2.0.18",
    "@prisma/client": "^5.0.0",
    "@supabase/auth-helpers-nextjs": "^0.7.4",
    "@supabase/supabase-js": "^2.32.0",
    "@trpc/client": "^10.34.0",
    "@trpc/server": "^10.34.0",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "@vercel/postgres": "^0.4.0",
    "autoprefixer": "10.4.14",
    "bcrypt": "^5.1.0",
    "client-only": "^0.0.1",
    "clsx": "^2.0.0",
    "controlled-form-hook": "^2.0.11",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "framer-motion": "^10.12.21",
    "hashids": "^2.3.0",
    "immer": "^10.0.2",
    "next": "13.4.10",
    "postcss": "8.4.26",
    "react": "18.2.0",
    "react-aria": "^3.26.0",
    "react-dom": "18.2.0",
    "server-only": "^0.0.1",
    "tailwindcss": "3.3.3",
    "tiny-validation": "^1.1.2",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "encoding": "^0.1.13",
    "prettier": "^3.0.1",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "prisma": "^5.0.0",
    "ts-node": "^10.9.1"
  }
}
